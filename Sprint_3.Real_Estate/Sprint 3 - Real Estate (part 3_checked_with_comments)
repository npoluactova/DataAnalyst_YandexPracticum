    data['first_day_exposition_test'] = data['first_day_exposition'].dt.date #–ø—Ä–∏–≤–æ–¥–∏–º –∫ –≤—Ä–µ–º–µ–Ω–Ω–æ–º—É —Ñ–æ—Ä–º–∞—Ç—É
